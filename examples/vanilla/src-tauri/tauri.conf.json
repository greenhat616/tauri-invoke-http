{
  "productName": "app",
  "version": "0.1.0",
  "identifier": "com.tauri.invoke_http",
  "build": {
    "frontendDist": "../public"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": ""
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "app",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
